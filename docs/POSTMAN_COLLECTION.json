							"mode": "raw",
							"raw": "{\n  \"message\": \"What about the pricing?\",\n  \"history\": [\n    {\n      \"role\": \"user\",\n      \"content\": \"Tell me about the features\"\n    },\n    {\n      \"role\": \"assistant\",\n      \"content\": \"The main features include AI capabilities, multi-language support, and real-time processing.\"\n    },\n    {\n      \"role\": \"user\",\n      \"content\": \"How does it work?\"\n    },\n    {\n      \"role\": \"assistant\",\n      \"content\": \"It works by processing documents and using RAG to provide accurate answers.\"\n    }\n  ]\n}"
