						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Response faster than 500ms (cache hit)\", function () {",
									"    pm.expect(pm.response.responseTime).to.be.below(500);",
									"});",
									"",
									"pm.test(\"Has response and sources\", function () {",
									"    var jsonData = pm.response.json();",
									"    pm.expect(jsonData.response).to.exist;",
									"    pm.expect(jsonData.sources).to.be.an('array');",
									"});"
								],
